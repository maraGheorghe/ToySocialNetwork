      <TextField fx:id="lastNameField" layoutX="181.0" layoutY="297.0" promptText="last name" stylesheets="@../../../css/logInSignUpTextFiels.css" />
      <PasswordField fx:id="passwordField" layoutX="181.0" layoutY="346.0" promptText="password" stylesheets="@../../../css/logInSignUpTextFiels.css" />
      <PasswordField fx:id="confirmPasswordField" layoutX="436.0" layoutY="346.0" promptText="confirm password" stylesheets="@../../../css/logInSignUpTextFiels.css" />
