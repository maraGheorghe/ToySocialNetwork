      <Label layoutX="80.0" layoutY="63.0" stylesheets="@../../../css/labelGreen.css" text="Please complete only what you want to change:" textFill="#384f25" />
      <Label layoutX="80.0" layoutY="27.0" stylesheets="@../../../css/labelMessages.css" text="Modify your profile" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="20.0" mnemonicParsing="false" onAction="#handleBackButton" stylesheets="@../../../css/superTransparentButton.css">
         <graphic>
            <ImageView fitHeight="34.0" fitWidth="33.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/back.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Circle fx:id="profilePicture" fill="#fefed5" layoutX="256.0" layoutY="146.0" radius="35.0" stroke="BLACK" strokeType="INSIDE" />
      <Button fx:id="profilePictureButton" layoutX="283.0" layoutY="168.0" mnemonicParsing="false" onAction="#handleProfilePicture" prefHeight="25.0" prefWidth="27.0" stylesheets="@../../../css/superTransparentButton.css">
