            <Font name="System Bold Italic" size="12.0" />
         </font></Text>
      <Text fill="#384f25" layoutX="63.0" layoutY="222.0" strokeType="OUTSIDE" strokeWidth="0.0" text="change email : ">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Text>
      <Text fill="#384f25" layoutX="39.0" layoutY="267.0" strokeType="OUTSIDE" strokeWidth="0.0" text="change first name : ">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Text>
      <Text fill="#384f25" layoutX="41.0" layoutY="314.0" strokeType="OUTSIDE" strokeWidth="0.0" text="change last name :">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Text>
      <Text fill="#384f25" layoutX="41.0" layoutY="363.0" strokeType="OUTSIDE" strokeWidth="0.0" text="change password :">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Text>
      <Text fill="#384f25" layoutX="19.0" layoutY="159.0" strokeType="OUTSIDE" strokeWidth="0.0" text="change profile picture :">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Text>
      <Button fx:id="okButton" layoutX="550.0" layoutY="437.0" mnemonicParsing="false" onAction="#handleOkButton" prefHeight="48.0" prefWidth="80.0" stylesheets="@../../../css/superTransparentButton.css">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="83.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/bifaOk.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <ImageView fitHeight="17.0" fitWidth="19.0" layoutX="407.0" layoutY="222.0" opacity="0.7" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/attentionMark.png" />
         </image>
      </ImageView>
      <Label fx:id="errorLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="140.0" layoutY="448.0" prefHeight="42.0" prefWidth="415.0" text="Label" textAlignment="CENTER" textFill="#cf1111" visible="false" />
   </children>
</AnchorPane>
