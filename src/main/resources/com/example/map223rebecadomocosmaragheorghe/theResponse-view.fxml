<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.TextFlow?>

<fx:root fx:id="paneRoot" prefHeight="70.0" prefWidth="80.0" style="-fx-background-radius: 30px; -fx-border-radius: 30px; -fx-padding: 5,10,5,10; -fx-background-color: linear-gradient(to right, rgba(143, 90, 61, 0.51), rgba(197, 147, 120, 0.49), rgba(143, 90, 61, 0.51));" type="AnchorPane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <Label fx:id="nameLabel" layoutX="20.0" layoutY="3.0" stylesheets="@../../../css/labelGreen.css" text="Name" />
      <TextFlow fx:id="textFlow" layoutX="7.0" layoutY="20.0">
         <children>
              <Label fx:id="messageLabel" alignment="CENTER_LEFT" contentDisplay="LEFT" stylesheets="@../../../css/labelResponse.css" text="Label" textAlignment="LEFT" />
         </children>
      </TextFlow>
      <Label fx:id="timeLabel" layoutX="30.0" layoutY="50.0" text="time" textFill="#777373" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" />
    </children>
</fx:root>
